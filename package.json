{
  "name": "mitum-sdk",
  "version": "0.2.0",
  "description": "Javascript SDK for mitum",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "test": "jest",
    "build": "npm run build:cjs & npm run build:esm",
    "build:cjs": "tsc --p ./cjs/tsconfig.json",
    "build:esm": "tsc --p ./esm/tsconfig.json",
    "check:orphans": "npx madge --orphans ./src/**/*",
    "check:circular": "npx madge --circular --image ./circular.svg ./src/**/*",
    "check:dependency": "npx madge --dot ./src/**/* > ./dot.gv "
  },
  "keywords": [
    "mitum",
    "protocon",
    "mitum-currency",
    "mitum-currency-extension"
  ],
  "license": "GPL-3.0-or-later",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProtoconNet/mitum-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/ProtoconNet/mitum-sdk-js/issues"
  },
  "homepage": "https://github.com/ProtoconNet/mitum-sdk-js#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@jest/globals": "^29.5.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@types/secure-random": "^1.1.0",
    "jest": "^29.5.0",
    "madge": "^6.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.0",
    "@noble/secp256k1": "^1.7.1",
    "axios": "^1.4.0",
    "big-integer": "^1.6.51",
    "bs58": "^5.0.0",
    "bs58check": "^3.0.1",
    "eccrypto-js": "^5.4.0",
    "ethereumjs-wallet": "^1.0.2",
    "int64-buffer": "^1.0.1",
    "js-sha3": "^0.8.0",
    "secure-random": "^1.1.2"
  }
}
